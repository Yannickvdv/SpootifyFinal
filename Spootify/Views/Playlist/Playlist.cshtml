@using Spootify.Models
@model List<Song>
@{
    ViewBag.Title = "Afspeellijst";
}

<h2>Afspeellijst</h2>
<div class="col-xs-6 col-md-3">
    @foreach (Song song in Model)
    {
        <a href="@Url.Action("Index", "Song", new {song.SongID})" class="thumbnail">
            <div class="caption">
                <h3>@song.Name</h3>
            </div>
        </a>
    }
</div>

